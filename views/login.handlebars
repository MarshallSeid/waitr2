    <!-- Header -->
    <header>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <img class="img-responsive" src="img/profile.png" alt="" width="15%" height="15%">
                    <div class="intro-text">
                        <span class="name">WAITR</span>
                        <hr class="star-light">
                        <span class="skills">Wait no more</span>
                    </div>
                </div>
                <div class="col-lg-8 col-lg-offset-2 text-center">
                    <br>
                    <h4>
                    <form name="login"> <font color="black">
                        Username &nbsp;&nbsp; <input type="text" name="userid"/><br><br>
                        Password &nbsp;&nbsp;<input type="password" name="pswrd"/>
                        <br><br>
                        <input type="button" onclick="check(this.form)" value="Login"/>
                        <input type="button" onclick="self.location='index'" value="Cancel"/>
                    </form>
                    <script language="javascript">
                        function check(form) { /*function to check userid & password*/
                            /*the following code checkes whether the entered userid and password are matching*/
                            if(form.userid.value == "waitr@gmail.com" && form.pswrd.value == "1234") {
                                open('home','_self')/*opens the target page while Id & password matches*/
                            }
                            else {
                                alert("Error Password or Username")/*displays error message*/
                            }
                        }
                    </script></font></h4>

                </div>
            </div>
        </div>
